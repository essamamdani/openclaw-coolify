# =============================================================================
# OpenClaw Environment Configuration
# =============================================================================
# Copy this file to .env and fill in your actual values
# NEVER commit .env to git - it contains secrets!
# =============================================================================

# =============================================================================
# PRIMARY AI PROVIDERS
# =============================================================================

# OpenAI (GPT-4, GPT-3.5, etc.)
OPENAI_API_KEY=

# Anthropic (Claude)
ANTHROPIC_API_KEY=

# Google (Gemini)
GEMINI_API_KEY=

# Mistral AI
MISTRAL_API_KEY=

# =============================================================================
# EXTENDED AI PROVIDERS (Optional)
# =============================================================================

# OpenRouter (unified API for multiple models)
OPENROUTER_API_KEY=

# xAI (Grok)
XAI_API_KEY=

# Groq (fast inference)
GROQ_API_KEY=

# Cerebras (ultra-fast inference)
CEREBRAS_API_KEY=

# Venice AI
VENICE_API_KEY=

# Moonshot AI
MOONSHOT_API_KEY=
MOONSHOT_BASE_URL=https://api.moonshot.ai/v1

# Kimi (Moonshot)
KIMI_API_KEY=
KIMI_BASE_URL=https://api.moonshot.ai/anthropic

# MiniMax
MINIMAX_API_KEY=

# Z.AI
ZAI_API_KEY=

# AI Gateway
AI_GATEWAY_API_KEY=

# OpenCode AI
OPENCODE_API_KEY=

# Synthetic AI
SYNTHETIC_API_KEY=

# Xiaomi AI
XIAOMI_API_KEY=

# Deepgram (speech-to-text)
DEEPGRAM_API_KEY=

# GitHub Copilot
COPILOT_GITHUB_TOKEN=

# =============================================================================
# AWS BEDROCK (Optional)
# =============================================================================
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_REGION=us-east-1
AWS_SESSION_TOKEN=
BEDROCK_PROVIDER_FILTER=anthropic

# =============================================================================
# OLLAMA (Self-hosted, Optional)
# =============================================================================
OLLAMA_BASE_URL=

# =============================================================================
# MODEL CONFIGURATION
# =============================================================================
# Set your preferred primary model (optional)
OPENCLAW_PRIMARY_MODEL=

# =============================================================================
# COMMUNICATION CHANNELS
# =============================================================================

# Telegram Bot
TELEGRAM_BOT_TOKEN=

# Discord Bot (optional)
DISCORD_BOT_TOKEN=

# Slack Bot (optional)
SLACK_BOT_TOKEN=
SLACK_APP_TOKEN=

# WhatsApp (optional)
WHATSAPP_ENABLED=false

# =============================================================================
# OTHER INTEGRATIONS
# =============================================================================

# ElevenLabs (text-to-speech)
ELEVENLABS_API_KEY=

# Google Maps
GOOGLE_MAPS_API_KEY=

# Brave Search (fallback search)
BRAVE_API_KEY=

# NanoBanana (optional)
NANOBANANA_API_KEY=

# =============================================================================
# GOOGLE CALENDAR (gog CLI)
# =============================================================================
GOOGLE_CALENDAR_CREDENTIALS_JSON=
GOG_KEYRING_PASSWORD=

# =============================================================================
# GATEWAY CONFIGURATION
# =============================================================================

# Gateway port (default: 18789)
OPENCLAW_GATEWAY_PORT=18789

# Trusted proxies (Coolify network CIDR)
# Use 10.0.0.0/8 to trust all 10.x.x.x addresses
GATEWAY_TRUSTED_PROXIES=10.0.0.0/8

# mDNS discovery mode (minimal, full, or off)
OPENCLAW_MDNS_MODE=minimal

# =============================================================================
# BROWSER AUTOMATION (Optional)
# =============================================================================

# Browser CDP URL (default: http://browser:9222)
BROWSER_CDP_URL=http://browser:9222

# Browser profile name
BROWSER_DEFAULT_PROFILE=openclaw

# Enable JavaScript evaluation in browser
BROWSER_EVALUATE_ENABLED=true

# Snapshot mode (efficient or full)
BROWSER_SNAPSHOT_MODE=efficient

# Browser timeouts (milliseconds)
BROWSER_REMOTE_TIMEOUT_MS=1500
BROWSER_REMOTE_HANDSHAKE_TIMEOUT_MS=3000

# =============================================================================
# HOOKS SYSTEM (Optional)
# =============================================================================

# Enable custom webhook integrations
HOOKS_ENABLED=false

# Path to hooks directory
HOOKS_PATH=/root/openclaw-workspace/hooks

# =============================================================================
# ADVANCED CONFIGURATION
# =============================================================================

# Custom APT packages to install (comma-separated)
OPENCLAW_DOCKER_APT_PACKAGES=

# Timezone
TZ=Etc/UTC

# OpenClaw beta features
OPENCLAW_BETA=false

# Sandbox container mode
SANDBOX_CONTAINER=true

# =============================================================================
# DEPLOYMENT (Coolify)
# =============================================================================

# Port (internal)
PORT=18789

# Node environment
NODE_ENV=production

# =============================================================================
# GIT INTEGRATION (Optional)
# =============================================================================

# GitHub token for git operations
GITHUB_TOKEN=

# Git user configuration
GITHUB_USERNAME=
GITHUB_EMAIL=

# =============================================================================
# CLOUDFLARE TUNNEL (Optional)
# =============================================================================

# Cloudflare Tunnel token for public access
CF_TUNNEL_TOKEN=

# =============================================================================
# VERCEL DEPLOYMENT (Optional)
# =============================================================================

# Vercel deployment credentials
VERCEL_TOKEN=
VERCEL_ORG_ID=
VERCEL_PROJECT_ID=